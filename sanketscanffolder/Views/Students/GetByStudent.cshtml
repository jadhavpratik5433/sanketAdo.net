@model IEnumerable<sanketscanffolder.Models.Semester>

<p>
    <a asp-controller="Semesters" asp-action="Create">Create New</a>
    <h3>@TempData["StudentName"].ToString()</h3>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Subjet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mark1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subjet2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mark2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Total)
            </th>
            @* <th>
                @Html.DisplayNameFor(model => model.studId)
            </th> *@
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Subjet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mark1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subjet2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mark2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
           @*  <td>
                @Html.DisplayFor(modelItem => item.studId)
            </td> *@
            <td>
                    <a asp-controller="Semesters" asp-action="Edit" asp-route-id="@item.SemesterId">Edit</a> |
                    @* <a asp-controller="Semesters" asp-action="Details" asp-route-id="@item.SemesterId">Details</a> | *@
                    <a asp-controller="Semesters"  asp-action="Delete" asp-route-id="@item.SemesterId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
